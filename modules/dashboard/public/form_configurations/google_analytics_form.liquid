---
name: google_analytics_form
resource: modules/dashboard/google_analytics
resource_owner: anyone
redirect_to: "/dashboard/google_analytics"
flash_notice: "You've successfully set Google Analytics"
fields:
  properties:
    tracking_id:
      validation: {}
default_payload: ''
---
{% form html-novalidate: false %}
  {% parse_json "utils" %}
    {% include 'modules/utils/init' %}
  {% endparse_json %}
  {% include utils.form_errors %}
  {% include utils.input with form.fields.properties.tracking_id, type: "text", label: "Tracking ID" %}
  {% include utils.submit_tag with "Save", class: "btn btn-primary" %}
{% endform %}
