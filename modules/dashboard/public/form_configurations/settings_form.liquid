---
name: settings_form
resource: Customization
resource_owner: anyone
redirect_to: "/dashboard/google_analytics"
flash_notice: "You've successfully updated project settings"
fields:
  properties:
    project_name:
      validation: {}
default_payload: ''
---
{% form html-novalidate: false %}
  {% parse_json "utils" %}
    {% include 'modules/utils/init' %}
  {% endparse_json %}
  {% include utils.form_errors %}
  {% include utils.input with form.fields.properties.project_name, type: "text" %}
  {% include utils.submit_tag with "Save", class: "btn btn-primary" %}
{% endform %}
